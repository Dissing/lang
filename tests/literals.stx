extern printf :: (format: *constchar, args: ...) -> i32

main :: () {
  u: u32 = 42;
  printf("%u\n", u);
  i: i32 = -12;
  printf("%d\n", i);
  c: char = 'X';
  printf("%c\n", c);
  s: *char = "Are you suggesting that coconuts migrate?";
  printf("%s\n", s);
  l: u64 = 17179869184;
  printf("%lu\n", l);
}
