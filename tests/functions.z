extern printf :: (format: *constchar, args: ...) -> i32

A :: () {
  printf("A\n");
}

B :: (a: u32, b: u32) -> u32 {
  printf("B: %u %u\n", a, b);
  return a + b;
}

C :: (a: *u32, b: u32) {
  *a = 12;
  b = 13;
}

D :: (n: u32) -> u32 {
  if n == 1 || n == 2  return 1;
  else  return D(n-1) + D(n-2);
}

main :: () {
  A();

  b: u32 = B(2, 3);
  printf("%u\n", b);

  x: u32 = 42;
  y: u32 = 42;
  C(&x, y);
  printf("%u %u\n", x, y);

  f: u32 = D(22);
  printf("%u\n", f);
}
