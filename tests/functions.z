
fn A() {
  printf("A\n");
}

fn B(a: u32, b: u32) -> u32 {
  printf("B: %u %u\n", a, b);
  return a + b;
}

fn C(a: *u32, b: u32) {
  *a = 12;
  b = 13;
}

fn D(n: u32) -> u32 {
  if n == 1 || n == 2  return 1;
  else  return D(n-1) + D(n-2);
}

fn main() {
  A();

  var b: u32 = B(2, 3);
  printf("%u\n", b);

  var x: u32 = 42;
  var y: u32 = 42;
  C(&x, y);
  printf("%u %u\n", x, y);

  var f: u32 = D(22);
  printf("%u\n", f);
}
