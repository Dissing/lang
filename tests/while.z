extern fn printf(format: *constchar, args: ...) -> i32

fn main() {
  var i: i32 = 0;
  while i < 6 {
    printf("%u", i);
    i = i + 1;
  }
  printf("\n");


  while i >= 1 {
    printf("%u", i);
    i = i - 1;
  }
  printf("\n");

  while i < 10 {
    i = i + 1;
    if i == 2 {
      continue;
    }
    else if i == 6  break;
    printf("%u", i);
  }
  printf("\n");
}
